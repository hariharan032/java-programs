import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int p=0;
        for(int i=0;i<a;i++){
            p=p+b;
        }
        System.out.println(p);
    }
}
