import java.util.Scanner;
class helloworld{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
       int []a=new int[4];
       int sum=0;
       for(int i=0;i<a.length;i++)
           a[i]=sc.nextInt();
       
       for(int i=0;i<a.length;i++){
          sum=sum+a[i];
      }
      for(int i=0;i<a.length;i++){
          a[i]=sum-a[i];
      }
      for(int i=0;i<a.length;i++){
          System.out.println(a[i]+" ");
      }
        
  }
}
